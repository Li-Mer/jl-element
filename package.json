{
  "name": "@jinli6386/element",
  "version": "1.0.0",
  "description": "Vue3 components library",
  "type": "module",
  "author": "Li Jin",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/jl-element.umd.cjs",
  "module": "./dist/es/jl-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/jl-element.js",
      "require": "./dist/umd/jl-element.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.2.0",
    "@fortawesome/free-solid-svg-icons": "^7.2.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.23"
  },
  "peerDependencies": {
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitepress-demo-preview/component": "^2.6.2",
    "@vitepress-demo-preview/plugin": "^1.4.1",
    "@vue-macros/volar": "^3.1.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "~1.42.0",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.42.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "~5.9.3",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^1.0.0-alpha.65",
    "vitest": "^4.0.18",
    "vue": "^3.5.27",
    "vue-tsc": "^3.0.8"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
